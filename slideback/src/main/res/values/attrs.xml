<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="4" />
            <enum name="bottom" value="8" />
        </attr>
        <attr name="slide_shadow_left" format="reference" />
        <attr name="slide_shadow_right" format="reference" />
        <attr name="slide_shadow_top" format="reference" />
        <attr name="slide_shadow_bottom" format="reference" />
    </declare-styleable>

    <attr name="SlideLayoutStyle" format="reference" />
</resources>